<p style="color: green"><%= notice %></p>



<table class="table table-striped table-bordered table-hover">
<thead class="table-dark">
    <tr>
      <th scope="col">First name</th>
      <th scope="col">Last name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div id="tests">
    <%= render @test %>
    </div>
  </tbody>
</table>
<div>
<%= link_to "Back to tests", tests_path , class:"btn btn-secondary"%>


  <%= link_to "Edit this test", edit_test_path(@test) , class:"btn btn-secondary"%> 
  <%= button_to "Destroy this test", @test, method: :delete, class:"btn btn-secondary" %>

</div>
